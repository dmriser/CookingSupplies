#!/bin/tcsh -f

#source /site/common_environment/set_environment /site/common_environment/jlab_default_settings.dat clas -v
setenv CLAS_CALDB_RUNINDEX calib_user.RunIndexe1f
echo $CLAS_CALDB_RUNINDEX
echo $CLAS_PARMS


alias kjuser_ana /lustre/expphy/volatile/clas/clas12/fcao/data/clas6/e1f/cookScripts/kjuser_ana 			# Makes an alias to the executable files 
alias nt22maker /lustre/expphy/volatile/clas/clas12/fcao/data/clas6/e1f/cookScripts/nt22maker_dvcs			# Makes an alias to the executable files 

mv datafile.bos Input
kjuser_ana -t frank_recsis_e1f_data.tcl >& /dev/null  	 # This line cooks the raw bos file, Input, and makes a file named cooked.bos. 
nt22maker						 # This line finds the files n10 files that are named histfile and converts them to n22 files
#echo "nt22maker done!"
h2root dstfile.hbook h22file.root 			# This line takes the nt22 files and converts them into root files
#echo "h2root done!"
